.page-id-21 
	header
		height 600px
		background-size cover
		background-repeat no-repeat
		background-position center
		position relative

		
		h2
			padding-top 230px
			width 70%
			margin-left 15%
			text-align center
			font-family plantinBold
			color #fff
			
	.section_1 
		border-bottom 1px solid #D3CFD8
		background white
		width 90%
		margin-left 5%
		margin-right 5%
		

		.section_1_p
			width 95%
			margin-top 35px	
			margin-right 5%	
			color #737373
			font-family plantinRegular
			font-size 18px
			
	.ourTeam
		width 90%
		margin-left 5%
		margin-right 5%
		padding-top 5%
		padding-bottom 10px
		overflow hidden
		h2
				width 55%
				float right
				font-family plantinRegular
				font-weight 300
				font-size 28px
			
			
				
			
		

	.staffSection
		width 90%
		margin-left 5%
		margin-right 5%
		border-bottom 1px solid #D3CFD8
		padding-top 20px
		
		.staffLeft
			overflow hidden
			width 45%
			float left
					
			.staffQuote
				width 45%
				float left	
				margin-left 3%
				margin-right 5%
				p
					font-family plantinItalic
					font-weight 500
					font-size 20px
					width 100%
		
		.staffRight
			width 50%
			padding-bottom 25px
			overflow hidden
			
			
			.nameTitle p
				font-family whitneyMedium
				color #737373
				
				span
					color black
					
			
			.staffParagraph
				p
					padding-bottom 25px
					margin-bottom 0
					color #737373
					font-family whitneyRegular
					font-size 18px
					
			

			.contactInformation
				
				overflow hidden
				width 80%
				
				
				p
					margin-bottom 0
					font-family whitneyMedium
					span
						font-weight 700
						font-family whitneyBold
					
		
		img
			background red
			width 175px
			height 175px
			float left

@media (max-width 920px)

	.page-id-21
		section.staffSection
			overflow hidden
			.staffLeft
				width 35%
				float left
				
				.staffQuote
					width 90%
					margin-left 0
					p
						width 175px
						padding-top 15px
						margin-left 0
						font-size 17px
			.staffRight
				width 65%			
						
	

			
@media (max-width 650px)
	.page-id-21 
		section.staffSection
			.staffLeft
				width 100% !important
				.staffQuote
					width 45%
					margin-top 5%
					margin-left 5%
					p
						width 100%
			.staffRight
				width 100%	
				padding-top 5%
				.nameTitle
					margin-bottom 10px
				
		.ourTeam
			h2
				width 100%
				text-align center
				
@media (max-width 610px)
	.page-id-21
		section.staffSection
			.staffLeft
				.staffQuote
					width 100%	
					margin-top 3%
					margin-left 0	
				
				.staffImage
					height 175px
					width 175px
					margin 0 auto	
					

					
@media (max-width 525px)
	.page-id-21
		section.staffSection
			.staffLeft
				.staffQuote
					margin-top 0
					


@media (max-width 435px)
	.page-id-21
		section.staffSection
			.staffLeft
				text-align center
				.staffQuote
					width 100% 	
					margin-left 0
					p
						margin-bottom 15px;
					
				.staffImage
					height 175px
					width 175px
					margin 0 auto	
			
			.staffRight
				padding-top 0		


				
			

		
			
			

			

			

						
		


			
		


			
	